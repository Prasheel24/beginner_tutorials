<launch>
<arg name="frequency" default="10" />
<node
 pkg="beginner_tutorials"
 type="talker"
 name="talkerNode"
 args="$(arg frequency)"
 required="true"
 output="screen"/>
<node
 pkg="beginner_tutorials"
 type="listener"
 name="listenerNode"
 output="screen"
 launch-prefix="gnome-terminal --command"/>
</launch>
